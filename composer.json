{
    "name": "ideallystudio/magento2-merchant-toolkit",
    "description": "Magento 2 admin enhancements to preview products, categories and CMS pages per store view.",
    "type": "magento2-module",
    "version": "1.1.0",
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=7.4",
        "magento/framework": "^103.0",
        "magento/module-catalog": "*",
        "magento/module-cms": "*",
        "magento/module-store": "*",
        "magento/module-ui": "*",
        "magento/module-url-rewrite": "*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "IdeallyStudio\\MerchantToolkit\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "app/code/IdeallyStudio/MerchantToolkit",
                "IdeallyStudio/MerchantToolkit"
            ]
        ]
    }
}
